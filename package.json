{
  "name": "geekonomy-project",
  "version": "1.0.0",
  "description": "All the BookCollections and User Athentication funcionalities will be handeled in this microservice",
  "main": "src/app.ts",
  "scripts": {
    "start": "npx tsc && node build/app.js",
    "build": "rimraf ./build && tsc",
    "development": "nodemon",
    "create-db": "npx sequelize-cli db:create",
    "create-seed": "npx sequelize-cli seed:generate --name",
    "seed-all": "npx sequelize-cli db:seed:all",
    "generate-migration": "npx sequelize-cli migration:generate --name",
    "migrate": "npx sequelize-cli db:migrate",
    "undo-migrate": "npx sequelize-cli db:migrate:undo",
    "undo-migrate-all": "npx sequelize-cli db:migrate:undo:all",
    "test": "jest --forceExit --detectOpenHandles  --watchAll --maxWorkers=1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "joi": "^17.9.2",
    "joi-password-complexity": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "rimraf": "^5.0.1",
    "sequelize": "^6.31.1",
    "sequelize-cli": "^6.6.0",
    "standard-http-error": "^2.0.1",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.18",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
